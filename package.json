{
    "name": "osmium",
    "description": "Node.js bindings to Osmium",
    "url": "https://github.com/osmcode/node-osmium",
    "homepage": "http://osmcode.org/node-osmium",
    "author": "Dane Springmeyer <springmeyer>",
    "contributors": [
        "Jochen Topf <joto>"
    ],
    "version": "0.1.3",
    "main": "./lib/osmium.js",
    "bugs": {
        "email": "dane@mapbox.com",
        "url": "https://github.com/osmcode/node-osmium/issues"
    },
    "keywords": [
        "osmium",
        "osm",
        "pbf",
        "changeset"
    ],
    "repository": {
        "type": "git",
        "url": "git://github.com/osmcode/node-osmium.git"
    },
    "binary": {
        "module_name": "osmium",
        "module_path" : "./lib/binding/",
        "host"        : "https://mapbox-node-binary.s3.amazonaws.com",
        "remote_path" : "./{module_name}/v{version}",
        "package_name": "{node_abi}-{platform}-{arch}.tar.gz"
    },
    "dependencies": {
        "node-pre-gyp": "~0.5.10"
    },
    "bundledDependencies":["node-pre-gyp"],
    "devDependencies": {
        "mocha": "*"
    },
    "licenses": [ { "type": "Boost" } ],
    "engines": {
        "node": ">= 0.6.13 < 0.11.0"
    },
    "scripts": {
        "install": "node-pre-gyp install --fallback-to-build",
        "test": "mocha -R spec"
    }
}
